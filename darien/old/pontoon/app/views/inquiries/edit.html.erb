<h1>Editing inquiry</h1>

<% form_for(@inquiry) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :person_id %><br />
    <%= f.text_field :person_id %>
  </p>
  <p>
    <%= f.label :department_id %><br />
    <%= f.text_field :department_id %>
  </p>
  <p>
    <%= f.label :message_text %><br />
    <%= f.text_area :message_text %>
  </p>
  <p>
    <%= f.label :person_phone %><br />
    <%= f.text_field :person_phone %>
  </p>
  <p>
    <%= f.label :graphic_arts_person %><br />
    <%= f.text_field :graphic_arts_person %>
  </p>
  <p>
    <%= f.label :created_on %><br />
    <%= f.datetime_select :created_on %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @inquiry %> |
<%= link_to 'Back', inquiries_path %>