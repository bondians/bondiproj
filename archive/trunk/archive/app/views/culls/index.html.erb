<table style= background-color: #777; height: 20px;" border="1" cellpadding="2" cellspacing="1">
  <tr>
    <th>Culled</th>
    <th>File</th>
    <th>Erased?</th>
    <th>Reason</th>
  </tr>

<% @culls.each do |cull| %>
  <tr>
    <td><%=h cull.created_at %></td>
    <td><%=h cull.file %></td>
    <% if cull.gone %>
      <td color: #700>YES</td>
    <% else %>
      <td color: #070>NO</td>
    <% end >
    <td><%=h cull.reason %></td>
  </tr>
<% end %>
</table>
